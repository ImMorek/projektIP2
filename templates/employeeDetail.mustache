{{#employee}}
<h1>Detail zaměstnance {{name}} {{surname}}</h1>
<dl>
    <dt>Jméno</dt>
    <dd>{{name}}</dd>

    <dt>Příjmení</dt>
    <dd>{{surname}}</dd>

    <dt>Pozice</dt>
    <dd>{{job}}</dd>

    <dt>Mzda</dt>
    <dd>{{wage}}</dd>

    <dt>Místnost</dt>
    <dd>{{room}}</dd>


    <dt>Klíče</dt>
        {{#rooms}}
        <dd>
            <a href='room.php?roomId={{room_id}}'>{{room_name}}</a> 
        </dd>
        {{/rooms}}
</dl>
{{/employee}}
